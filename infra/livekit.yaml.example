# LiveKit Server Configuration
# For development, use: --dev --bind 0.0.0.0 (no config file needed)
# For production, copy to livekit.yaml and run ./setup-secrets.sh to generate keys

port: 7880
rtc:
  udp_port: 7882
  tcp_port: 7881
  use_external_ip: true
keys:
  # Generated by setup-secrets.sh - NEVER commit real keys
  YOUR_API_KEY: YOUR_API_SECRET
